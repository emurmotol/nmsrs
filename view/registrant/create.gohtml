<form class="form-horizontal" action="/registrants" method="POST" id="create_registrant_form" enctype="multipart/form-data">
    <div class="tab-content">
        <div class="tab-pane active" role="tabpanel" id="step-1">
        <!--<div class="tab-pane" role="tabpanel" id="step-1">-->
            <div class="panel panel-default">
                <div class="panel-heading">Personal Information</div>
                <div class="panel-body">
                    <div class="form-group">
                        <label for="photo" class="col-lg-3 control-label">Photo</label>
                        <div class="col-lg-8">
                            <img src="{{config.default.photo.path}}" data-default-photo="{{config.default.photo.path}}" class="img-resopnsive img-rounded" id="preview" height="160" alt="Registrant Photo">
                            <input type="file" class="form-control" name="photo" id="photo" data-parsley-trigger="change" data-parsley-maxmegabytes="{{config.default.photo.maxSize}}" data-parsley-mimes="{{config.default.photo.mimes}}">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="family_name" class="col-lg-3 control-label">Family Name <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control uppercase" name="family_name" id="family_name" data-parsley-required="true" data-parsley-minlength="2" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="given_name" class="col-lg-3 control-label">Given Name <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control uppercase" name="given_name" id="given_name" data-parsley-required="true" data-parsley-minlength="2" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="middle_name" class="col-lg-3 control-label">Middle Name <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control uppercase" name="middle_name" id="middle_name" data-parsley-required="true" data-parsley-minlength="1" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="birthdate" class="col-lg-3 control-label">Birthdate <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control uppercase" name="birthdate" id="birthdate" data-parsley-required="true" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="password" class="col-lg-3 control-label">Password</label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control uppercase" name="password" id="password" data-parsley-minlength="3" data-parsley-trigger="change">
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Basic Information</div>
                <div class="panel-body">
                    <div class="form-group">
                        <label for="st_sub" class="col-lg-3 control-label">Street/Subdivision <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control uppercase" name="st_sub" id="st_sub" data-parsley-required="true" data-parsley-minlength="1" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="city_mun_id" class="col-lg-3 control-label">City/Municipality <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <select class="form-control" name="city_mun_id" id="city_mun_id" data-parsley-required="true" data-parsley-trigger="change" data-parsley-errors-container="#city_mun_id_err"></select>
                            <span id="city_mun_id_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="prov_id" class="col-lg-3 control-label">Province</label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control uppercase" name="prov_id" id="prov_id" data-parsley-minlength="2" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="brgy_id" class="col-lg-3 control-label">Barangay <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <select class="form-control" name="brgy_id" id="brgy_id" data-parsley-required="true" data-parsley-trigger="change" data-parsley-errors-container="#brgy_id_err" disabled></select>
                            <span id="brgy_id_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="place_of_birth" class="col-lg-3 control-label">Place Of Birth <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control uppercase" name="place_of_birth" id="place_of_birth" data-parsley-required="true" data-parsley-minlength="2" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="religion_id" class="col-lg-3 control-label">Religion <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <select class="form-control" name="religion_id" id="religion_id" data-parsley-required="true" data-parsley-trigger="change" data-parsley-errors-container="#religion_id_err"></select>
                            <span id="religion_id_err"></span>
                        </div>
                    </div>
                    <div class="form-group" id="civil_stat_radios">
                        <label for="civil_stat_id" class="col-lg-3 control-label">Civil Status <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            {{range .civilStats}}
                            <div class="radio">
                                <label>
                                    <input type="radio" name="civil_stat_id" id="civil_stat_{{.ID}}" value="{{.ID}}"{{if eq .ID 1}} data-parsley-required="true" data-parsley-trigger="change" data-parsley-errors-container="#civil_stat_id_err"{{end}}> {{.Name}}
                                </label>
                            </div>
                            {{end}}
                            <span id="civil_stat_id_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-8 col-lg-offset-3">
                            <input class="form-control uppercase" type="text" name="civil_stat_other" id="civil_stat_other" data-parsley-minlength="2" data-parsley-trigger="change" placeholder="Simplify" disabled>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="sex_id" class="col-lg-3 control-label">Sex <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            {{range .sexes}}
                            <label class="radio-inline">
                                <input type="radio" name="sex_id" id="sex_{{.ID}}" value="{{.ID}}"{{if eq .ID 1}} data-parsley-required="true" data-parsley-trigger="change" data-parsley-errors-container="#sex_id_err"{{end}}> {{.Name}}
                            </label>
                            {{end}}
                            <span id="sex_id_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="age" class="col-lg-3 control-label">Age</label>
                        <div class="col-lg-8">
                            <input type="number" class="form-control" name="age" id="age" data-parsley-min="18" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="height" class="col-lg-3 control-label">Height (cm) <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <input type="number" class="form-control" name="height" id="height" data-parsley-required="true" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="weight" class="col-lg-3 control-label">Weight (kg)</label>
                        <div class="col-lg-8">
                            <input type="number" class="form-control" name="weight" id="weight" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="landline_no" class="col-lg-3 control-label">Landline Number</label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control" name="landline_no" id="landline_no" data-parsley-type="digits" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="mobile_no" class="col-lg-3 control-label">Mobile Number</label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control" name="mobile_no" id="mobile_no" data-parsley-type="digits" data-parsley-trigger="change">
                        </div>
                    </div>
                    <div class="form-group{{with .createRegistrantForm.Errors.Email}} has-error{{end}}">
                        <label for="email" class="col-lg-3 control-label">Email</label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control" name="email" id="email" value="{{.createRegistrantForm.Email}}" data-parsley-type="email" data-parsley-whitespace="trim" data-parsley-trigger="change" data-parsley-remote data-parsley-remote-validator="emailTaken" data-parsley-remote-message="">
                            {{with .createRegistrantForm.Errors.Email}}<span class="help-block">{{.}}</span>{{end}}
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Employment</div>
                <div class="panel-body">
                    <div class="form-group" id="emp_stat_radios">
                        <label for="emp_stat_id" class="col-lg-3 control-label">Employment Status <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            {{range .empStats}}
                            <div class="radio">
                                <label>
                                    <input type="radio" name="emp_stat_id" id="emp_stat_{{.ID}}" value="{{.ID}}"{{if eq .ID 1}} data-parsley-required="true" data-parsley-trigger="change" data-parsley-errors-container="#emp_stat_id_err"{{end}}> {{.Name}}
                                </label>
                            </div>
                            {{end}}
                            <span id="emp_stat_id_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-8 col-lg-offset-3">
                            <select class="form-control" name="un_emp_stat_id" id="un_emp_stat_id" data-parsley-trigger="change" data-parsley-errors-container="#un_emp_stat_id_err" disabled></select>
                            <span id="un_emp_stat_id_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-8 col-lg-offset-3">
                            <select class="form-control" name="toc_id" id="toc_id" data-parsley-trigger="change" data-parsley-errors-container="#toc_id_err" disabled></select>
                            <span id="toc_id_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-8 col-lg-offset-3">
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox" id="alfw" name="alfw" value="false"> Actively looking for work?
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="pref_occ_ids" class="col-lg-3 control-label">Preferred Occupation <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <select class="form-control" name="pref_occ_ids" id="pref_occ_ids" multiple="multiple" data-parsley-validate-if-empty data-parsley-mincheck="1" data-parsley-trigger="change" data-parsley-errors-container="#pref_occ_ids_err"></select>
                            <span id="pref_occ_ids_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="pref_local_loc_id" class="col-lg-3 control-label">Local Location <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <select class="form-control" name="pref_local_loc_id" id="pref_local_loc_id" data-parsley-required="true" data-parsley-trigger="change" data-parsley-errors-container="#pref_local_loc_id_err"></select>
                            <span id="pref_local_loc_id_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="pref_overseas_loc_id" class="col-lg-3 control-label">Overseas Location <span class="asterisk text-danger">*</span></label>
                        <div class="col-lg-8">
                            <select class="form-control" name="pref_overseas_loc_id" id="pref_overseas_loc_id" data-parsley-required="true" data-parsley-trigger="change" data-parsley-errors-container="#pref_overseas_loc_id_err"></select>
                            <span id="pref_overseas_loc_id_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="passport_no" class="col-lg-3 control-label">Passport Number</label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control uppercase" name="passport_no" id="passport_no" data-parsley-trigger="change" data-parsley-type="alphanum">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="pned" class="col-lg-3 control-label">Expiry Date</label>
                        <div class="col-lg-8">
                            <input type="text" class="form-control uppercase" name="pned" id="pned" data-parsley-trigger="change">
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Disability</div>
                <div class="panel-body">
                    <div class="form-group" id="disability_radios">
                        <label for="disability_id" class="col-lg-3 control-label">Disability</label>
                        <div class="col-lg-8">
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox" id="disabled" name="disabled" value="false"> Disabled?
                                </label>
                            </div>
                            {{range .disabilities}}
                            <div class="radio">
                                <label>
                                    <input type="radio" name="disability_id" id="disability_{{.ID}}" value="{{.ID}}"{{if eq .ID 1}} data-parsley-trigger="change" data-parsley-errors-container="#disability_id_err"{{end}} disabled> {{.Name}}
                                </label>
                            </div>
                            {{end}}
                            <span id="disability_id_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-8 col-lg-offset-3">
                            <input class="form-control uppercase" type="text" name="disability_other" id="disability_other" data-parsley-minlength="2" data-parsley-trigger="change" placeholder="Simplify" disabled>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Language</div>
                <div class="panel-body">
                    <div class="form-group">
                        <label for="language_ids" class="col-lg-3 control-label">Language</label>
                        <div class="col-lg-8">
                            <select class="form-control" name="language_ids" id="language_ids" multiple="multiple" data-parsley-mincheck="1" data-parsley-trigger="change" data-parsley-errors-container="#language_ids_err"></select>
                            <span id="language_ids_err"></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--<div class="tab-pane active" role="tabpanel" id="step-2">-->
        <div class="tab-pane" role="tabpanel" id="step-2">
            <div class="panel panel-default">
                <div class="panel-heading clearfix">
                    <span class="pull-left">Formal Education</span>
                    <span class="pull-right">
                        <button type="button" class="btn btn-success btn-xs" id="add_formal_edu_button" role="button">
                            <i class="fa fa-plus"></i> Add
                        </button>
                        <button type="button" class="btn btn-danger btn-xs" id="delete_formal_edu_button" role="button" disabled>
                            <i class="fa fa-trash-o"></i> Delete
                        </button>
                    </span>
                </div>
                <div class="table-responsive">
                    <table class="table table-striped table-hover" id="formal_edu_table">
                        <thead>
                            <tr>
                                <th><input type="checkbox" id="select_all_formal_edu"></th>
                                <th>Highest Grade Completed</th>
                                <th>Course/Degree</th>
                                <th>School/University</th>
                                <th>Year Graduated</th>
                                <th>Last Attended</th>
                                <th class="text-center">Edit</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
                <input type="hidden">
            </div>
        </div>
        <div class="tab-pane" role="tabpanel" id="step-3">
        <!--<div class="tab-pane active" role="tabpanel" id="step-3">-->
            <div class="panel panel-default">
                <div class="panel-heading clearfix">
                    <span class="pull-left">Professional License</span>
                    <span class="pull-right">
                        <button type="button" class="btn btn-success btn-xs" id="add_pro_license_button" role="button">
                            <i class="fa fa-plus"></i> Add
                        </button>
                        <button type="button" class="btn btn-danger btn-xs" id="delete_pro_license_button" role="button" disabled>
                            <i class="fa fa-trash-o"></i> Delete
                        </button>
                    </span>
                </div>
                <div class="table-responsive">
                    <table class="table table-striped table-hover" id="pro_license_table">
                        <thead>
                            <tr>
                                <th><input type="checkbox" id="select_all_pro_license"></th>
                                <th>Title</th>
                                <th>Expiry Date</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
                <input type="hidden">
            </div>
        </div>
        <div class="tab-pane" role="tabpanel" id="step-4">
        <!--<div class="tab-pane active" role="tabpanel" id="step-4">-->
            <div class="panel panel-default">
                <div class="panel-heading clearfix">
                    <span class="pull-left">Eligibility</span>
                    <span class="pull-right">
                        <button type="button" class="btn btn-success btn-xs" id="add_elig_button" role="button">
                            <i class="fa fa-plus"></i> Add
                        </button>
                        <button type="button" class="btn btn-danger btn-xs" id="delete_elig_button" role="button" disabled>
                            <i class="fa fa-trash-o"></i> Delete
                        </button>
                    </span>
                </div>
                <div class="table-responsive">
                    <table class="table table-striped table-hover" id="elig_table">
                        <thead>
                            <tr>
                                <th><input type="checkbox" id="select_all_elig"></th>
                                <th>Title</th>
                                <th>Year Taken</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
                <input type="hidden">
            </div>
        </div>
        <div class="tab-pane" role="tabpanel" id="step-5">
        <!--<div class="tab-pane active" role="tabpanel" id="step-5">-->
            <div class="panel panel-default">
                <div class="panel-heading clearfix">
                    <span class="pull-left">Vocational/Technical Training And Relevant Experience</span>
                    <span class="pull-right">
                        <button type="button" class="btn btn-success btn-xs" id="add_vttare_button" role="button">
                            <i class="fa fa-plus"></i> Add
                        </button>
                        <button type="button" class="btn btn-danger btn-xs" id="delete_vttare_button" role="button" disabled>
                            <i class="fa fa-trash-o"></i> Delete
                        </button>
                    </span>
                </div>
                <div class="table-responsive">
                    <table class="table table-striped table-hover" id="vttare_table">
                        <thead>
                            <tr>
                                <th><input type="checkbox" id="select_all_vttare"></th>
                                <th>Name Of Training</th>
                                <th>Skills Acquired</th>
                                <th>Period Of Training/Experience</th>
                                <th>Certificate Received</th>
                                <th>Issuing School/Agency</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
                <input type="hidden">
            </div>
        </div>
        <div class="tab-pane" role="tabpanel" id="step-6">
        <!--<div class="tab-pane active" role="tabpanel" id="step-6">-->
            <div class="panel panel-default">
                <div class="panel-heading clearfix">
                    <span class="pull-left">Certificate Of Competence</span>
                    <span class="pull-right">
                        <button type="button" class="btn btn-success btn-xs" id="add_coc_button" role="button">
                            <i class="fa fa-plus"></i> Add
                        </button>
                        <button type="button" class="btn btn-danger btn-xs" id="delete_coc_button" role="button" disabled>
                            <i class="fa fa-trash-o"></i> Delete
                        </button>
                    </span>
                </div>
                <div class="table-responsive">
                    <table class="table table-striped table-hover" id="coc_table">
                        <thead>
                            <tr>
                                <th><input type="checkbox" id="select_all_coc"></th>
                                <th>Title</th>
                                <th>Rating</th>
                                <th>Issued By</th>
                                <th>Date Issued</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
                <input type="hidden">
            </div>
        </div>
        <div class="tab-pane" role="tabpanel" id="step-7">
        <!--<div class="tab-pane active" role="tabpanel" id="step-7">-->
            <div class="panel panel-default">
                <div class="panel-heading clearfix">
                    <span class="pull-left">Work Experience</span>
                    <span class="pull-right">
                        <button type="button" class="btn btn-success btn-xs" id="add_work_exp_button" role="button">
                            <i class="fa fa-plus"></i> Add
                        </button>
                        <button type="button" class="btn btn-danger btn-xs" id="delete_work_exp_button" role="button" disabled>
                            <i class="fa fa-trash-o"></i> Delete
                        </button>
                    </span>
                </div>
                <div class="table-responsive">
                    <table class="table table-striped table-hover" id="work_exp_table">
                        <thead>
                            <tr>
                                <th><input type="checkbox" id="select_all_work_exp"></th>
                                <th>Name Of Company/Firm</th>
                                <th>Address</th>
                                <th>Position Held</th>
                                <th>From</th>
                                <th>To</th>
                                <th>Related To Formal Education</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
                <input type="hidden">
            </div>
        </div>
        <div class="tab-pane" role="tabpanel" id="step-8">
        <!--<div class="tab-pane active" role="tabpanel" id="step-8">-->
            <div class="panel panel-default">
                <div class="panel-heading clearfix">
                    <span class="pull-left">Other Skills Aquired Without Formal Training</span>
                </div>
                <div class="panel-body">
                    <div class="form-group">
                        <label for="skill_ids" class="col-lg-3 control-label">Skills </label>
                        <div class="col-lg-8">
                            <select class="form-control" name="skill_ids" id="skill_ids" multiple="multiple" data-parsley-mincheck="1" data-parsley-trigger="change" data-parsley-errors-container="#skill_ids_err"></select>
                            <span id="skill_ids_err"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-8 col-lg-offset-3">
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox" id="skill_nl" value="false"> Skill not listed?
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-lg-8 col-lg-offset-3">
                            <input class="form-control uppercase" type="text" name="skill_other" id="skill_other" data-parsley-minlength="2" data-parsley-trigger="change" placeholder="Skill" disabled>
                        </div>
                    </div>
                </div>
                <input type="hidden">
            </div>
        </div>
        <div class="tab-pane" role="tabpanel" id="step-9">
        <!--<div class="tab-pane active" role="tabpanel" id="step-9">-->
            <div class="panel panel-default">
                <div class="panel-heading clearfix">
